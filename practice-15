
let i = 1;
while(i < 10){
    console.log(i + " ");
    i++
}

// do...while
let k = 1
do{
    console.log(k + " ");
    k++
} while (k < 10);


// for
for(let i = 1; i < 100; i) {
    if(i === 10) break;
    console.log(i + " ");
}

for(let k = 1; k < 11; k++) { 
    if(k === 10 ) continue;
    console.log(k + " ");
}
